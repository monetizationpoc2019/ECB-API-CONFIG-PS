<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
  <version ptype="INTEGER">2</version>

  <ScheduledAdapters>

    <Adapter>
      <Name>CreditCardDunningAdapter</Name>
      <DisplayName>Failed Credit Card Payment Dunning</DisplayName>
      <ClassID>MetraTech.UsageServer.Adapters.GenericWorkflowAdapter, MetraTech.UsageServer.Adapters</ClassID>
      <ConfigFile>CreditCardDunningAdapter.xml</ConfigFile>
      <ActivationDate>10/1/2002 00:00:00</ActivationDate>
      <Description>
        The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
      </Description>
      <LocalizedItems>
        <item lang="FR">
          <DisplayName>Relace des paiements par carte de crédit en échec</DisplayName>
          <Description>
            [FR] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
          </Description>
        </item>
        <item lang="IT">
          <DisplayName>[IT] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [IT] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
        <item lang="ES">
          <DisplayName>[ES] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [ES] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
        <item lang="DE">
          <DisplayName>[DE] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [DE] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
        <item lang="es-MX">
          <DisplayName>[es-MX] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [es-MX] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
        <item lang="pt-BR">
          <DisplayName>Falha na cobrança do pagamento de cartão de crédito</DisplayName>
          <Description>
            [pt-BR] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
          </Description>
        </item>
        <item lang="en-GB">
          <DisplayName>[en-GB] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [en-GB] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
        <item lang="JA">
          <DisplayName>[JA] Failed Credit Card Payment Dunning</DisplayName>
          <Description>
            [JA] The adapter resubmits failed credit card payments on a periodic basis for a configured number of retries.
       
          </Description>
        </item>
      </LocalizedItems>
      <RecurrencePattern>
        <SystemCycle>Daily</SystemCycle>
      </RecurrencePattern>
    </Adapter>

    <Adapter>
      <Name>ACHInquiryAdapter</Name>
      <DisplayName>ACH Inquiry Adapter</DisplayName>
      <ClassID>MetraTech.UsageServer.Adapters.GenericWorkflowAdapter, MetraTech.UsageServer.Adapters</ClassID>
      <ConfigFile>ACHInquiryAdapter.xml</ConfigFile>
      <ActivationDate>10/1/2002 00:00:00</ActivationDate>
      <Description>
        This adapter checks to see if a given ach transaction has cleared.
       
      </Description>
      <LocalizedItems>
        <item lang="FR">
          <DisplayName>Adaptateur de requête ACH</DisplayName>
          <Description>
            Cet adaptateur vérifie si une transaction ACH spécifique a été autorisée.
       
          </Description>
        </item>
        <item lang="IT">
          <DisplayName>[IT] ACH Inquiry Adapter</DisplayName>
          <Description>
            [IT] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
        <item lang="ES">
          <DisplayName>[ES] ACH Inquiry Adapter</DisplayName>
          <Description>
            [ES] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
        <item lang="DE">
          <DisplayName>[DE] ACH Inquiry Adapter</DisplayName>
          <Description>
            [DE] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
        <item lang="es-MX">
          <DisplayName>[es-MX] ACH Inquiry Adapter</DisplayName>
          <Description>
            [es-MX] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
        <item lang="pt-BR">
          <DisplayName>Adaptador de Consulta ACH</DisplayName>
          <Description>
            Este adaptador verifica se uma determinada transação ach cancelou.
          </Description>
        </item>
        <item lang="en-GB">
          <DisplayName>[en-GB] ACH Inquiry Adapter</DisplayName>
          <Description>
            [en-GB] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
        <item lang="JA">
          <DisplayName>[JA] ACH Inquiry Adapter</DisplayName>
          <Description>
            [JA] This adapter checks to see if a given ach transaction has cleared.
       
          </Description>
        </item>
      </LocalizedItems>
      <RecurrencePattern>
        <SystemCycle>Daily</SystemCycle>
      </RecurrencePattern>
    </Adapter>

    <Adapter>
      <Name>ExecutePaymentsAdapter</Name>
      <DisplayName>Submit Scheduled Payments to Payment Processors</DisplayName>
      <ClassID>MetraTech.UsageServer.Adapters.GenericWorkflowAdapter,MetraTech.UsageServer.Adapters</ClassID>
