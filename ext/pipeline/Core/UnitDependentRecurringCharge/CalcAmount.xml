CREATE PROCEDURE CalcAmount @BaseAmount DECIMAL @UnitAmount DECIMAL @UnitValue DECIMAL @RCAmount DECIMAL OUTPUT
AS
-- straight copy - might be something else later
SET @RCAmount = @BaseAmount + @UnitAmount*@UnitValue
	  
